{
  "name": "emmacli",
  "description": "Command line interface to manage emma entities",
  "version": "0.0.5",
  "author": "Emma",
  "bin": {
    "emmacli": "bin/run"
  },
  "bugs": "https://github.com/MandarinSolutions/emma-cli/issues",
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.0",
    "axios": "^0.24.0",
    "ssl-root-cas": "^1.3.1",
    "qs": "^6.10.3",
    "js-yaml": "^4.1.0",
    "cli-ux": "^6.0.9"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "shx": "^0.3.4",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/config",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/MandarinSolutions/emma-cli",
  "keywords": [
    "cli",
    "emma",
    "emmacli"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "emmacli",
    "dirname": "emmacli",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "macos": {
      "identifier": "com.emma.cli"
    },
    "topics": {
      "api": { "description": "API limits information" },
      "api:limits": { "description": "API limits information" },
      "applications": { "description": "Manage applications" },
      "auth": { "description": "Authorize in emma platform" },
      "clusters": { "description": "Manage k8s clusters" },
      "commons": { "description": "Common entities operations" },
      "networks": { "description": "Manage networks security groups" },
      "networks:security-groups": { "description": "Manage networks security groups" },
      "spots": { "description": "Manage spots" },
      "vms": { "description": "Manage vms" },
      "products": { "description": "List products" }
    }
  },
  "repository": "MandarinSolutions/emma-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint src/** --ext .ts --config .eslintrc",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "test": "mocha --forbid-only \\\"test/**/*.test.ts\\\"",
    "version": "oclif readme && git add README.md",
    "packWin": "oclif pack win",
    "packMacos": "oclif pack macos",
    "packDeb": "oclif pack deb"
  },
  "types": "dist/index.d.ts"
}
